<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="newamp">
    <static>false</static>
    <link name="hull">
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_hull.obj</uri>
            <scale>1 1 1</scale> <!-- Adjust scale if needed -->
          </mesh>
        </geometry>
            <material>
      <script>
        <uri>file://model://newamp/meshes/textures/</uri>
        <name>amp_hull</name>
      </script>
    </material>
      </visual>
      <collision name="hull">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_hull.obj</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>

      </collision>
    </link>

    <link name="front_right_wheel">
    <pose>0 0 -0.05 0 0 0</pose>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_wheel.obj</uri>
            <scale>1 1 1</scale> <!-- Adjust scale if needed -->
          </mesh>
        </geometry>
            <material>
      <script>
        <uri>file://model://newamp/meshes/textures/</uri>
        <name>amp_wheel</name>
      </script>
    </material>
      </visual>
      <collision name="front_right_wheel">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_wheel.obj</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
    </link>

     <link name="front_left_wheel">
    <pose>0 1 -0.05 0 0 0</pose>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_wheel.obj</uri>
            <scale>1 1 1</scale> <!-- Adjust scale if needed -->
          </mesh>
        </geometry>
            <material>
      <script>
        <uri>file://model://newamp/meshes/textures/</uri>
        <name>amp_wheel</name>
      </script>
    </material>
      </visual>
      <collision name="front_left_wheel">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_wheel.obj</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
    </link>

     <link name="back_right_wheel">
    <pose>-1 0 -0.05 0 0 0</pose>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_wheel.obj</uri>
            <scale>1 1 1</scale> <!-- Adjust scale if needed -->
          </mesh>
        </geometry>
            <material>
      <script>
        <uri>file://model://newamp/meshes/textures/</uri>
        <name>amp_wheel</name>
      </script>
    </material>
      </visual>
      <collision name="back_right_wheel">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_wheel.obj</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
    </link>

     <link name="back_left_wheel">
    <pose>-1 1 -0.05 0 0 0</pose>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_wheel.obj</uri>
            <scale>1 1 1</scale> <!-- Adjust scale if needed -->
          </mesh>
        </geometry>
            <material>
      <script>
        <uri>file://model://newamp/meshes/textures/</uri>
        <name>amp_wheel</name>
      </script>
    </material>
      </visual>
      <collision name="back_left_wheel">
        <geometry>
          <mesh>
            <uri>model://newamp/meshes/amp_wheel.obj</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
    </link>


    <joint name="front_left_wheel_joint" type="fixed">
  <parent>hull</parent>
  <child>front_left_wheel</child>
  <axis>
    <xyz>0 1 0</xyz> <!-- Rotation axis -->
  </axis>
</joint>


  <joint name="front_right_wheel_joint" type="fixed">
  <parent>hull</parent>
  <child>front_right_wheel</child>
  <axis>
    <xyz>0 1 0</xyz> <!-- Rotation axis -->
  </axis>
</joint>

<joint name="back_right_wheel_joint" type="fixed">
  <parent>hull</parent>
  <child>back_right_wheel</child>
  <axis>
    <xyz>0 1 0</xyz> <!-- Rotation axis -->
  </axis>
</joint>

<joint name="back_left_wheel_joint" type="fixed">
  <parent>hull</parent>
  <child>back_left_wheel</child>
  <axis>
    <xyz>0 1 0</xyz> <!-- Rotation axis -->
  </axis>
</joint>


  </model>
</sdf>
