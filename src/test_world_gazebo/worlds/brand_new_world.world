    <population name="tall_box_population_left">
      <model name="tall_box">
        <include>
          <static>true</static>
          <uri>model://oak_tree</uri>
          <pose>0 0 0 0 0 0</pose>
        </include>
      </model>
      <pose>-15 0 0 0 0 0</pose>
      <box>
        <size>15 125 0.01</size>
      </box>
      <model_count>40</model_count>
      <distribution>
        <type>random</type>
      </distribution>
    </population>

